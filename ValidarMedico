int ValidarMedico(char *CRM){

    FILE *point;
    medico medicos;

    point = fopen("medicos.bin", "rb");

    while(fread(&medicos, sizeof(medico), 1, point) == 1){
        if(strcmp(medicos.CRM, CRM) == 0){
            printf("\nMedico encontrado!");
            return 1;
        }
    }

    printf("\nMedico nao encontrado!");
    fclose(point);
    return 0;
}
